{
  "name": "ng-webpack-demo",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=production node server.js",
    "build": "cross-env NODE_ENV=production webpack",
    "prestart": "npm install && npm run test-single-run && npm run build",
    "dev": "concurrently -r 'npm run dev:client' 'npm run dev:server'",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server -d --inline --progress --no-info",
    "dev:server": "cross-env NODE_ENV=development node server.js",
    "test": "cross-env NODE_ENV=test karma start karma.conf.js",
    "test-single-run": "karma start karma.conf.js --single-run",
    "clean": "rimraf public/"
  },
  "devDependencies": {
    "angular": "^1.2.29",
    "angular-mocks": "^1.5.7",
    "angular-ui-router": "^0.3.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "bootstrap": "^3.3.6",
    "bootstrap-webpack": "0.0.5",
    "browser-launcher": "^1.0.0",
    "concurrently": "^2.2.0",
    "cors": "^2.7.1",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "eslint": "^3.1.0",
    "eslint-config-angular": "^0.5.0",
    "eslint-plugin-angular": "^1.3.0",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "imports-loader": "^0.6.5",
    "jasmine-core": "^2.4.1",
    "jquery": "^3.1.0",
    "karma": "^1.1.1",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.1.0",
    "karma-eslint": "^2.1.0",
    "karma-jasmine": "^1.0.2",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.7.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "morgan": "^1.7.0",
    "ng-annotate-webpack-plugin": "^0.1.3",
    "protractor": "^4.0.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.3",
    "style-loader": "^0.13.1",
    "to-string-loader": "^1.1.4",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-split-by-path": "^0.1.0"
  }
}
